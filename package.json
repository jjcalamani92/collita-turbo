{
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:tailwind": "concurrently 'next dev --turbo' 'npm run tailwind --watch'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@tanstack/react-query": "4.13.4",
    "@trpc/client": "10.0.0-proxy-beta.26",
    "@trpc/next": "10.0.0-proxy-beta.26",
    "@trpc/react-query": "10.0.0-proxy-beta.26",
    "@trpc/server": "10.0.0-proxy-beta.26",
    "clsx": "1.2.1",
    "concurrently": "7.5.0",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "next": "latest",
    "next-auth": "4.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.0",
    "superjson": "1.11.0",
    "sweetalert2": "11.6.4",
    "use-count-up": "3.0.1",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
